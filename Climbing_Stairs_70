int climbStairs(int n) {
    if (n == 0) return 1;  
    if (n == 1) return 1;  
    int ways[n + 1];
    ways[0] = 1;  
    ways[1] = 1;  
    for (int i = 2; i <= n; i++) {  
        ways[i] = ways[i - 1] + ways[i - 2];
    }
    return ways[n]; 
int main() {
    int n;
    printf("Enter number of steps: ");
    scanf("%d", &n);
    int result = climbStairs(n);
    printf("Number of distinct ways to climb %d steps: %d\n", n, result);
    return 0;
}
}
