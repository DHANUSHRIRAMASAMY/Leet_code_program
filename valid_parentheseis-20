bool isValid(char* s) {
    int len=strlen(s);
    char* stack=(char*)malloc((len+1)*sizeof(char));
    int top=-1;
    for(int i=0;s[i]!='\0';i++)
{
    if(s[i]=='('||s[i]=='{'||s[i]=='['){
        stack[++top]=s[i];
    }
    else{
        if(top==-1)return false;
        else if(s[i]==')' && stack[top]!='('||s[i]=='}'&& stack[top]!='{' ||s[i]==']'&&stack[top]!='['){
            return false;
        }
        top--;
    }
}
bool res=top==-1;
free(stack);
return res;
}
